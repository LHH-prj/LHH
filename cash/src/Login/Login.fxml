<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.control.PasswordField?>


<GridPane prefHeight="116.0" prefWidth="281.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8" fx:controller="Login.Controller">
   <columnConstraints>
      <ColumnConstraints />
      <ColumnConstraints maxWidth="138.0" minWidth="91.0" prefWidth="137.0" />
      <ColumnConstraints maxWidth="301.0" minWidth="190.0" prefWidth="190.0" />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
   </rowConstraints>
   <children>
      <VBox alignment="CENTER" prefHeight="107.0" prefWidth="91.0" spacing="20.0" GridPane.columnIndex="1" GridPane.rowIndex="1">
         <children>
            <Label text="ID" />
            <Label text="PASSWORD" />
         </children>
      </VBox>
      <VBox alignment="CENTER" prefHeight="155.0" prefWidth="320.0" spacing="10.0" GridPane.columnIndex="2" GridPane.rowIndex="1">
         <children>
            <TextField fx:id="userId">
               <VBox.margin>
                  <Insets left="10.0" right="10.0" />
               </VBox.margin>
            </TextField>
            <PasswordField fx:id="userPw">
               <VBox.margin>
                  <Insets left="10.0" right="10.0" />
               </VBox.margin>
            </PasswordField>
         </children>
      </VBox>
      <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" spacing="50.0" GridPane.columnIndex="2" GridPane.rowIndex="2">
         <children>
            <Button onAction="#CancelProc" fx:id="buttonCancle" mnemonicParsing="false" text="취소" />
            <Button onAction="#OpenMember" fx:id="buttonMemberShip" mnemonicParsing="false" text="회원가입" />
         </children>
      </HBox>
      <Button onAction="#LoginProc" fx:id="buttonOk" mnemonicParsing="false" text="확인" GridPane.columnIndex="1" GridPane.rowIndex="2">
         <GridPane.margin>
            <Insets left="17.0" />
         </GridPane.margin>
      </Button>
   </children>
</GridPane>
